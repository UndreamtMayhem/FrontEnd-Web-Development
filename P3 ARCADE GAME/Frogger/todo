
//sprite width and height

var player = {x: 5, y: 5, width: 50, height: 50}
var oth_obj = {x: 20, y: 10, width: 10, height: 10}

//bounding box
//template
if (player.x < oth_obj.x + oth_obj.width &&
   player.x + player.width > oth_obj.x &&
   player.y < oth_obj.y + oth_obj.height &&
   player.height + player.y > oth_obj.y) {
    // collision detected!
}

// filling in the values =>

if (5 < 30 &&
    55 > 20 &&
    5 < 20 &&
    55 > 10) {
    // collision detected!
}

//circle collision

var circle1 = {radius: 20, x: 5, y: 5};
var circle2 = {radius: 12, x: 10, y: 5};

var dx = circle1.x - circle2.x;
var dy = circle1.y - circle2.y;
var distance = Math.sqrt(dx * dx + dy * dy);

if (distance < circle1.radius + circle2.radius) {
    // collision detected!
}




